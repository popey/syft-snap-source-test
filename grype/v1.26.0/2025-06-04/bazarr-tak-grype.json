0 by default disabling this feature.","cvss":[{"source":"nvd@nist.gov","type":"Primary","version":"3.1","vector":"CVSS:3.1/AV:N/AC:H/PR:N/UI:N/S:U/C:L/I:L/A:N","metrics":{"baseScore":4.8,"exploitabilityScore":2.3,"impactScore":2.6},"vendorMetadata":{}},{"source":"security-advisories@github.com","type":"Secondary","version":"3.1","vector":"CVSS:3.1/AV:N/AC:L/PR:N/UI:N/S:U/C:H/I:H/A:N","metrics":{"baseScore":9.1,"exploitabilityScore":3.9,"impactScore":5.2},"vendorMetadata":{}}],"epss":[{"cve":"CVE-2024-49768","epss":0.0002,"percentile":0.03726,"date":"2025-06-03"}]}],"matchDetails":[{"type":"exact-direct-match","matcher":"python-matcher","searchedBy":{"language":"python","namespace":"github:language:python","package":{"name":"waitress","version":"3.0.0"}},"found":{"versionConstraint":">=2.0.0,<3.0.1 (python)","vulnerabilityID":"GHSA-9298-4cf8-g4wj"},"fix":{"suggestedVersion":"3.0.1"}}],"artifact":{"id":"85eb79b91875038e","name":"waitress","version":"3.0.0","type":"python","locations":[{"path":"/Bazarr/libs/waitress-3.0.0.dist-info/METADATA","accessPath":"/Bazarr/libs/waitress-3.0.0.dist-info/METADATA","annotations":{"evidence":"primary"}},{"path":"/Bazarr/libs/waitress-3.0.0.dist-info/RECORD","accessPath":"/Bazarr/libs/waitress-3.0.0.dist-info/RECORD","annotations":{"evidence":"supporting"}},{"path":"/Bazarr/libs/waitress-3.0.0.dist-info/top_level.txt","accessPath":"/Bazarr/libs/waitress-3.0.0.dist-info/top_level.txt","annotations":{"evidence":"supporting"}}],"language":"python","licenses":["ZPL 2.1"],"cpes":["cpe:2.3:a:zope_foundation_and_contributors_project:python-waitress:3.0.0:*:*:*:*:*:*:*","cpe:2.3:a:zope_foundation_and_contributors_project:python_waitress:3.0.0:*:*:*:*:*:*:*","cpe:2.3:a:zope_foundation_and_contributorsproject:python-waitress:3.0.0:*:*:*:*:*:*:*","cpe:2.3:a:zope_foundation_and_contributorsproject:python_waitress:3.0.0:*:*:*:*:*:*:*","cpe:2.3:a:zope_foundation_and_contributors_project:waitress:3.0.0:*:*:*:*:*:*:*","cpe:2.3:a:zope_foundation_and_